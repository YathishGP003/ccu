<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical" >

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:overScrollMode="never">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <TableLayout
                android:id="@+id/ahu"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="30dp"
                android:layout_marginLeft="50dp"
                android:layout_marginRight="50dp"
                android:layout_marginTop="14dp"
                android:stretchColumns="0"
                android:visibility="visible">

                <TableRow
                    android:id="@+id/ahuHeader"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="2dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Equipment List"
                        android:textSize="@dimen/text_medium_xlarge"
                        android:visibility="invisible"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="   Selector List  item touch  "
                        android:textSize="@dimen/text_medium_xlarge"
                        android:visibility="invisible"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="Test Signal"
                        android:textSize="@dimen/text_medium_xlarge"
                        android:visibility="visible"/>

                </TableRow>

                <TableRow
                    android:id="@+id/ahuAnalog1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp">

                    <CheckBox
                        android:id="@+id/ahuAnalog1Cb"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingRight="14dp"
                        android:textSize="@dimen/text_small_xlarge"
                        android:layout_gravity="center_vertical"
                        android:text=" Cooling (Analog 1)" />

                    <!--<Spinner
                    android:id="@+id/ahuAnalog1Type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"/>-->

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="   Selector List  item touch  "
                        android:textSize="@dimen/text_medium_xlarge"
                        android:visibility="invisible"/>

                    <Spinner
                        android:id="@+id/ahuAnalog1Test"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"/>

                </TableRow>

                <TableRow
                    android:id="@+id/ahuAnalog2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp">

                    <CheckBox
                        android:id="@+id/ahuAnalog2Cb"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:paddingRight="14dp"
                        android:text="  Static Pressure (Analog 2)"
                        android:textSize="@dimen/text_small_xlarge"/>

                    <!--<Spinner
                    android:id="@+id/ahuAnalog2Type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|center" />-->

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="   Selector List  item touch  "
                        android:textSize="@dimen/text_medium_xlarge"
                        android:visibility="invisible"/>

                    <Spinner
                        android:id="@+id/ahuAnalog2Test"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"/>

                </TableRow>

                <TableRow
                    android:id="@+id/ahuAnalog3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp">

                    <CheckBox
                        android:id="@+id/ahuAnalog3Cb"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:paddingRight="14dp"
                        android:text=" Heating (Analog 3)"
                        android:textSize="@dimen/text_small_xlarge"/>

                    <!-- <Spinner
                    android:id="@+id/ahuAnalog3Type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"/>-->

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="   Selector List  item touch  "
                        android:textSize="@dimen/text_medium_xlarge"
                        android:visibility="invisible"/>
-->
                    <Spinner
                        android:id="@+id/ahuAnalog3Test"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"/>

                </TableRow>

                <TableRow
                    android:id="@+id/ahuAnalog4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp">

                    <CheckBox
                        android:id="@+id/ahuAnalog4Cb"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:paddingRight="14dp"
                        android:text="  CO2 (Analog 4)"
                        android:textSize="@dimen/text_small_xlarge"/>

                    <!--<Spinner
                    android:id="@+id/ahuAnalog2Type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|center" />-->

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="   Selector List  item touch  "
                        android:textSize="@dimen/text_medium_xlarge"
                        android:visibility="invisible"/>

                    <Spinner
                        android:id="@+id/ahuAnalog4Test"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"/>

                </TableRow>

                <TableLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:stretchColumns="0">

                    <TableRow
                        android:id="@+id/ahuRow"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginBottom="14dp"
                        android:layout_marginTop="14dp">

                        <ImageView
                            android:layout_width="fill_parent"
                            android:layout_height="1dp"
                            android:src="@color/progress_gray_dark"/>
                    </TableRow>
                </TableLayout>

                <GridLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="14dp"
                    android:layout_marginTop="14dp"
                    android:columnCount="4">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="0"
                        android:layout_gravity="left"
                        android:layout_marginBottom="14dp"
                        android:layout_marginTop="14dp"
                        android:layout_row="0"
                        android:text="Analog1AtMinCoolingSAT"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_small_xlarge"/>

                    <Spinner
                        android:id="@+id/ahuAnalog1Min"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_marginBottom="14dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="14dp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginBottom="14dp"
                        android:layout_marginLeft="14dp"
                        android:layout_marginTop="14dp"
                        android:text="Analog1AtMaxCoolingSAT"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_small_xlarge"
                        />

                    <Spinner
                        android:id="@+id/ahuAnalog1Max"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="3"
                        android:layout_marginBottom="14dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="14dp"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="0"
                        android:layout_gravity="left"
                        android:layout_marginBottom="14dp"
                        android:layout_marginTop="14dp"
                        android:layout_row="1"
                        android:text="Analog2AtMinStaticPressure"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_small_xlarge"/>

                    <Spinner
                        android:id="@+id/ahuAnalog2Min"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_marginBottom="14dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="14dp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginBottom="14dp"
                        android:layout_marginLeft="14dp"
                        android:layout_marginTop="14dp"
                        android:text="Analog2AtMaxStaticPressure"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_small_xlarge"/>

                    <Spinner
                        android:id="@+id/ahuAnalog2Max"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginBottom="14dp"
                        android:layout_marginTop="14dp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="0"
                        android:layout_gravity="left"
                        android:layout_marginBottom="14dp"
                        android:layout_marginTop="14dp"
                        android:layout_row="2"
                        android:text="Analog3AtMinHeating"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_small_xlarge"/>

                    <Spinner
                        android:id="@+id/ahuAnalog3Min"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_marginBottom="14dp"

                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="14dp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginBottom="14dp"
                        android:layout_marginLeft="40dp"
                        android:layout_marginTop="14dp"
                        android:text="Analog3AtMaxHeating"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_small_xlarge"/>

                    <Spinner
                        android:id="@+id/ahuAnalog3Max"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginBottom="14dp"
                        android:layout_marginTop="14dp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="0"
                        android:layout_gravity="left"
                        android:layout_marginBottom="14dp"
                        android:layout_marginTop="14dp"
                        android:layout_row="3"
                        android:text="Analog4AtMinCO2"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_small_xlarge"/>

                    <Spinner
                        android:id="@+id/ahuAnalog4Min"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_marginBottom="14dp"

                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="14dp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginBottom="14dp"
                        android:layout_marginLeft="40dp"
                        android:layout_marginTop="14dp"
                        android:text="Analog4AtMaxCO2"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_small_xlarge"/>

                    <Spinner
                        android:id="@+id/ahuAnalog4Max"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginBottom="14dp"
                        android:layout_marginTop="14dp"/>
                </GridLayout>
            </TableLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>