                        android:layout_marginTop="14dp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="0"
                        android:layout_gravity="left"
                        android:layout_marginBottom="14dp"
                        android:layout_marginTop="14dp"
                        android:layout_row="3"
