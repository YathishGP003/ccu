<resources>

    <declare-styleable name="SeekArc">
        <attr name="thumb" format="reference" />
        <attr name="thumbOffset" format="dimension" />
        <attr name="max" format="integer" />
        <attr name="progressWidth" format="dimension" />
        <attr name="arcWidth" format="dimension" />
        <attr name="progress" format="integer" />
        <attr name="rotation" format="integer" />
        <attr name="startAngle" format="integer" />
        <attr name="sweepAngle" format="integer" />
        <attr name="arcColor" format="color" />
        <attr name="progressColor" format="color" />
        <attr name="roundEdges" format="boolean" />
        <attr name="touchInside" format="boolean" />
        <attr name="touchOutSide" format="boolean" />
        <attr name="clockwise" format="boolean" />
        <attr name="progresstextsize" format="dimension"/>
        <attr name="outertextsize" format="dimension" />
        <attr name="statustextsize" format="dimension" />
        <attr name="userlimittextsize" format="dimension"/>
        <attr name="thumbcircletextsize" format="dimension"/>
        <attr name="thumbouterradius" format="dimension"/>
        <attr name="thumbinnerradius" format="dimension"/>
        <attr name="markertextheight" format="dimension"/>
        <attr name="statustextheight" format="dimension" />
        <attr name="markertextwidthx" format="dimension"/>
        <attr name="markertextwidthy" format="dimension"/>
        <attr name="statustextwidth" format="dimension"/>
        <attr name="statusoutsidetextwidth" format="dimension" />
        <attr name="statusoutsidetextheight" format="dimension" />
        <attr name="temptextwidth" format="dimension"/>
        <attr name="temptextheight" format="dimension"/>
        <attr name="thumbtextwidth" format="dimension"/>
        <attr name="thumbtextheight" format="dimension"/>
        <attr name="smallthumbradius" format="dimension"/>
        <attr name="thumbdifference" format="dimension"/>



    </declare-styleable>

    <declare-styleable name="SeekArcTheme">
        <attr name="seekArcStyle" format="reference"></attr>
    </declare-styleable>

    <!-- Declare custom theme attributes that allow changing which styles are
         used for button bars depending on the API level.
         ?android:attr/buttonBarStyle is new as of API 11 so this is
         necessary to support previous API levels. -->
    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="NonTempControl">

        <attr name="expand" format="boolean" />
        <attr name="equipType" format="integer" />

        <attr name="image" format="reference" />
        <attr name="imageViewExpanded" format="reference" />

        <attr name="piInput" format="reference" />
        <attr name="piOutput" format="reference" />

        <attr name="piInputText" format="string" />
        <attr name="piOutputText" format="string" />

        <attr name="piInputUnit" format="reference" />
        <attr name="piOutputUnit" format="reference" />

        <attr name="piInputUnitText" format="string" />
        <attr name="piOutputUnitText" format="string" />

        <attr name="emTotal" format="reference" />
        <attr name="emTotalUnit" format="reference" />

        <attr name="emTotalText" format="string" />
        <attr name="emTotalUnitText" format="string" />

        <attr name="emCurrent" format="reference" />
        <attr name="emCurrentUnit" format="reference" />

        <attr name="emCurrentText" format="string" />
        <attr name="emCurrentUnitText" format="string" />
    </declare-styleable>

    <!-- Number picker -->
    <attr name="numberPickerStyle" format="reference" />

    <declare-styleable name="SystemNumberPicker">
        <attr name="np_width" format="dimension" />
        <attr name="np_height" format="dimension" />
        <attr name="np_divider" format="reference" />
        <attr name="np_dividerColor" format="color" />
        <attr name="np_dividerDistance" format="dimension" />
        <attr name="np_dividerThickness" format="dimension" />
        <attr name="np_fadingEdgeEnabled" format="boolean" />
        <attr name="np_fadingEdgeStrength" format="float" />
        <attr name="np_formatter" format="string" />
        <attr name="np_hideWheelUntilFocused" format="boolean" />
        <attr name="np_lineSpacingMultiplier" format="float" />
        <attr name="np_max" format="integer" />
        <attr name="np_maxFlingVelocityCoefficient" format="integer" />
        <attr name="np_min" format="integer" />
        <attr name="np_order" format="enum">
            <enum name="ascending" value="0" />
            <enum name="descending" value="1" />
        </attr>
        <attr name="np_orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="np_scrollerEnabled" format="boolean" />
        <attr name="np_selectedTextAlign" format="enum">
            <enum name="selectedTextAlignRight" value="0" />
            <enum name="selectedTextAlignCenter" value="1" />
            <enum name="selectedTextAlignLeft" value="2" />
        </attr>
        <attr name="np_selectedTextColor" format="color" />
        <attr name="np_selectedTextSize" format="dimension" />
        <attr name="np_selectedTextStrikeThru" format="boolean" />
        <attr name="np_selectedTextUnderline" format="boolean" />
        <attr name="np_textAlign" format="enum">
            <enum name="textAlignRight" value="0" />
            <enum name="textAlignCenter" value="1" />
            <enum name="textAlignLeft" value="2" />
        </attr>
        <attr name="np_textColor" format="color" />
        <attr name="np_textSize" format="dimension" />
        <attr name="np_textStrikeThru" format="boolean" />
        <attr name="np_textUnderline" format="boolean" />
        <attr name="np_typeface" format="string" />
        <attr name="np_value" format="integer" />
        <attr name="np_wheelItemCount" format="integer" />
        <attr name="np_wrapSelectorWheel" format="boolean" />
    </declare-styleable>

    <declare-styleable name="NumberPicker">
        <attr name="nps_width" format="dimension" />
        <attr name="nps_height" format="dimension" />
        <attr name="nps_accessibilityDescriptionEnabled" format="boolean" />
        <attr name="nps_divider" format="reference" />
        <attr name="nps_dividerType" format="enum">
            <enum name="side_lines" value="0" />
            <enum name="underline" value="1" />
        </attr>
        <attr name="nps_dividerColor" format="color" />
        <attr name="nps_dividerDistance" format="dimension" />
        <attr name="nps_dividerLength" format="dimension" />
        <attr name="nps_dividerThickness" format="dimension" />
        <attr name="nps_fadingEdgeEnabled" format="boolean" />
        <attr name="nps_fadingEdgeStrength" format="float" />
        <attr name="nps_formatter" format="string" />
        <attr name="nps_hideWheelUntilFocused" format="boolean" />
        <attr name="nps_itemSpacing" format="dimension" />
        <attr name="nps_lineSpacingMultiplier" format="float" />
        <attr name="nps_max" format="integer" />
        <attr name="nps_maxFlingVelocityCoefficient" format="integer" />
        <attr name="nps_min" format="integer" />
        <attr name="nps_order" format="enum">
            <enum name="ascending" value="0" />
            <enum name="descending" value="1" />
        </attr>
        <attr name="nps_orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="nps_scrollerEnabled" format="boolean" />
        <attr name="nps_selectedTextAlign" format="enum">
            <enum name="selectedTextAlignRight" value="0" />
            <enum name="selectedTextAlignCenter" value="1" />
            <enum name="selectedTextAlignLeft" value="2" />
        </attr>
        <attr name="nps_selectedTextColor" format="color" />
        <attr name="nps_selectedTextSize" format="dimension" />
        <attr name="nps_selectedTextStrikeThru" format="boolean" />
        <attr name="nps_selectedTextUnderline" format="boolean" />
        <attr name="nps_selectedTypeface" format="string" />
        <attr name="nps_textAlign" format="enum">
            <enum name="textAlignRight" value="0" />
            <enum name="textAlignCenter" value="1" />
            <enum name="textAlignLeft" value="2" />
        </attr>
        <attr name="nps_textColor" format="color" />
        <attr name="nps_textSize" format="dimension" />
        <attr name="nps_textStrikeThru" format="boolean" />
        <attr name="nps_textUnderline" format="boolean" />
        <attr name="nps_typeface" format="string" />
        <attr name="nps_value" format="integer" />
        <attr name="nps_wheelItemCount" format="integer" />
        <attr name="nps_wrapSelectorWheel" format="boolean" />
    </declare-styleable>
</resources>
